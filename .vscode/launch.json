{
  "version": "1.0.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug APP-API",
      "runtimeExecutable": "nx",
      "runtimeArgs": ["run", "app-api:serve", "--", "--inspect-brk", "--watch"],
      "autoAttachChildProcesses": true,
      "restart": true,
      "sourceMaps": true,
      "stopOnEntry": false,
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}/packages/app-api"
    }
  ]
}
